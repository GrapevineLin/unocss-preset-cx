{
  "name": "unocss-preset-cx",
  "type": "module",
  "version": "0.0.12",
  "description": "蝉选 preset for [UnoCSS](https://github.com/unocss/unocss).",
  "author": "GrapevineLin <linyudeng1@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/GrapevineLin/unocss-preset-cx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrapevineLin/unocss-preset-cx.git",
    "directory": "packages/preset-cx"
  },
  "bugs": {
    "url": "https://github.com/GrapevineLin/unocss-preset-cx/issues"
